# /run/state is mounted by state_disk_mount.service (not via fstab) before local-fs
/run/state/var           /var               none    defaults,bind,x-mount.mkdir,x-systemd.requires-mounts-for=/run/state                0    0
/run/state/kubernetes    /etc/kubernetes    none    defaults,bind,x-mount.mkdir,x-systemd.requires-mounts-for=/run/state                0    0
#/run/state/etccni        /etc/cni/          none    defaults,bind,x-mount.mkdir,x-systemd.requires-mounts-for=/run/state                0    0
/run/state/opt           /opt               none    defaults,bind,x-mount.mkdir,x-systemd.requires-mounts-for=/run/state                0    0
/run/state/etciscsi      /etc/iscsi         none    defaults,bind,x-mount.mkdir,x-systemd.requires-mounts-for=/run/state                0    0
